{
    "name": "lupus/api",
    "type": "project",
    "description": "API endpoint for the popular game Lupus in Tabula",
    "keywords": [
        "lupus",
        "tabula",
        "api",
        "game"
    ],
    "homepage": "https://github.com/Tbaile/LupusWeb",
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "ext-mysqli": "*",
        "ext-pdo": "*",
        "ext-pdo_mysql": "*",
        "ext-redis": "*",
        "fruitcake/laravel-cors": "2.2.0",
        "guzzlehttp/guzzle": "7.4.2",
        "laravel/framework": "9.5.1",
        "laravel/sanctum": "2.14.2",
        "laravel/tinker": "2.7.1",
        "spatie/laravel-enum": "3.0.1"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "2.12.3",
        "brianium/paratest": "6.4.3",
        "fakerphp/faker": "1.19.0",
        "mockery/mockery": "1.5.0",
        "nunomaduro/collision": "6.1.0",
        "nunomaduro/larastan": "2.1.2",
        "pestphp/pest": "1.21.2",
        "pestphp/pest-plugin-faker": "1.0.0",
        "pestphp/pest-plugin-laravel": "1.2.0",
        "pestphp/pest-plugin-parallel": "1.0.0",
        "phpunit/phpunit": "9.5.19",
        "spatie/laravel-ignition": "1.1.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "phpstan": "vendor/bin/phpstan analyze -c phpstan.neon.dist",
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
